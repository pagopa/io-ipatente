{
  "name": "io-ipatente-vehicles",
  "author": {
    "name": "PagoPA"
  },
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "test": "yarn test:bff && yarn test:fe",
    "test:bff": "vitest run --config ./vitest/vitest.config.node.mjs",
    "test:fe": "vitest run --config ./vitest/vitest.config.react.mjs",
    "test:coverage": "yarn coverage:bff && yarn coverage:fe",
    "coverage:bff": "vitest run --config ./vitest/vitest.config.node.mjs --coverage",
    "coverage:fe": "vitest run --config ./vitest/vitest.config.react.mjs --coverage",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "npm-run-all 'generate:*'",
    "generate:openapi-schema": "rimraf src/generated && mkdir -p src/generated && openapi-zod-client ./openapi.yaml -o src/generated/openapi.ts -t template-bff.hbs --additional-props-default-value=false"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@io-ipatente/core": "workspace:*",
    "@io-ipatente/ui": "workspace:*",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.5",
    "@mui/system": "^5.14.5",
    "@pagopa/mui-italia": "^1.5.0",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.56.2",
    "@zodios/core": "^10.9.6",
    "axios": "^1.7.7",
    "i18next": "^23.15.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.15",
    "next-auth": "^5.0.0-beta.21",
    "next-i18next": "^15.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@pagopa/eslint-config": "^4.0.1",
    "@tanstack/eslint-plugin-query": "^5.57.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jsdom": "^25.0.0",
    "msw": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "openapi-zod-client": "^1.18.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5",
    "vite": "^5.4.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
