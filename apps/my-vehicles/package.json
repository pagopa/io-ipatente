{
  "name": "my-vehicles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "test": "yarn test:bff && yarn test:fe",
    "test:bff": "vitest run --config ./vitest/vitest.config.node.mjs",
    "test:fe": "vitest run --config ./vitest/vitest.config.react.mjs",
    "coverage": "yarn coverage:bff && yarn coverage:fe",
    "coverage:bff": "vitest run --config ./vitest/vitest.config.node.mjs --coverage",
    "coverage:fe": "vitest run --config ./vitest/vitest.config.react.mjs --coverage",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@pagopa/eslint-config": "^4.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jsdom": "^25.0.0",
    "msw": "^2.4.4",
    "prettier": "^3.3.3",
    "typescript": "^5",
    "vite": "^5.4.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
